<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/bg_temp01"
    tools:context=".historyChart">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"/>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="10.15dp"
        android:background="@drawable/historycontainer_01_01"/>

    <ImageView
        android:id="@+id/chart"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_weight="3"

        android:background="@drawable/historycontainer_01_02"
        android:gravity="center_horizontal" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="10.15dp"
        android:background="@drawable/historycontainer_01_03"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"/>






</LinearLayout>